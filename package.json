{
  "name": "discordjsv14-template",
  "version": "0.0.0-development",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node -r ts-node/register ./src/index.ts",
    "dev": "nodemon",
    "update": "ts-node src/commands/index.ts",
    "build": "tsc",
    "lint": "eslint src environment.ts",
    "lint:fix": "eslint src environment.ts --fix"
  },
  "author": "expdani",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "2.4.0",
    "discord-api-types": "0.37.110",
    "discord.js": "14.16.3",
    "dotenv": "16.4.7",
    "eslint": "9.16.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  },
  "devDependencies": {
    "nodemon": "3.1.7"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
